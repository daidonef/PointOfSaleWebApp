<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="com.daidonef.pointofsale.AHProducts" table="ahproducts">
	<id name="ahProductsID" column="ahproductsid" type="int">
		<generator class="increment"></generator>
	</id>
	<property name="historyID" column="historyid" type="int"></property>
	<property name="productID" column="productid" type="int"></property>
	<property name="productName" column="productname" type="string"></property>
	<property name="price" column="price" type="double"></property>
	<property name="quantity" column="quantity" type="int"></property>
	
</class>
</hibernate-mapping>