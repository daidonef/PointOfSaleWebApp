<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="com.daidonef.pointofsale.AccountHistory" table="accounthistory">
	<id name="historyID" column="historyid" type="int">
		<generator class="increment"></generator>
	</id>
	<property name="accountID" column="accountid" type="int"></property>
	<property name="grandTotal" column="grandtotal" type="double"></property>
	<property name="paymentType" column="paymentType" type="string"></property>
	<property name="cashPayment" column="cashpayment" type="double"></property>
	<property name="change" column="change" type="double"></property>
	<property name="creditCardNumber" column="creditCardNumber" type="long"></property>
	<property name="securityCode" column="securitycode" type="int"></property>
	<property name="checkNumber" column="checknumber" type="int"></property>
	
</class>
</hibernate-mapping>